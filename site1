<script>
if(jQuery('.listagem-cursos').length > 0) {
    jQuery('.listagem-cursos.ativo .fusion-column-wrapper .fusion-builder-row').eq(0).addClass('ativo');
    jQuery('.tabopcao').click(function(e){
        var index = jQuery(".taboptions li" ).index( this );
        jQuery(this).siblings().removeClass('ativado');
        jQuery(this).addClass('ativado');
       
        jQuery('.listagem-cursos.ativo .fusion-column-wrapper .fusion-builder-row').removeClass('ativo');
        jQuery('.listagem-cursos.ativo').find('.fusion-column-wrapper .fusion-builder-row').eq(index).addClass('ativo');
        
        calcValores();
    });

    var $anterior = jQuery('.nav-anterior');
    var $proximo = jQuery('.nav-proximo');
    var $container, $curso, $index, $visivel, $total, $distancia;

    function calcValores() {
        $container = jQuery(document).find('.listagem-cursos.ativo .fusion-column-wrapper .ativo');
        $curso = $container.find('.capa-cursos').eq(0).width();
        $curso += 30;
        
        $index = Math.floor($container.width() / $curso);
        $visivel = Math.floor($container.width() / $curso);
        $total = $container.find('.capa-cursos').length - 1;
        $distancia = 0;
        
        $anterior.addClass("inativo");
        $proximo.removeClass("inativo");
    } calcValores();
    
    $anterior.on("click", function(e){
        e.preventDefault();

        if($index > $visivel) {
            $proximo.removeClass("inativo");
            $index--;
            $distancia += $curso;
            $container.css("transform","translateX("+$distancia+"px)");
            
            if($index == $visivel) {
                $anterior.addClass("inativo");
            }
        }
    });
    
    $proximo.on("click", function(e){
        e.preventDefault();
        
        if($index <= $total) {
            $anterior.removeClass("inativo");
            $distancia -= $curso;
            $container.css("transform","translateX("+$distancia+"px)");
            
            if($index == $total) {
                $proximo.addClass("inativo");
            }
            $index++;
        }
    });
    
}

if(jQuery('.abas-duas-colunas').length > 0) {
    jQuery('.abas-duas-colunas').each(function() {
        var $container = jQuery(this);
        $container.find('.fusion-li-item').eq(0).addClass("active");
        if(jQuery(this).hasClass("abas-filhas")) {
            $container.find('.fusion-li-item').on("click", function(e){
                e.stopPropagation();
                $container.find('.fusion-li-item').removeClass("active");
                jQuery(this).addClass("active");
            });
            $container.find('.fusion-li-item ul li').on("click", function(e){
                e.stopPropagation();
                var desc = $container.find('.abas-duas-colunas-direita .fusion-builder-row'),
                    index = $container.find(".abas-duas-colunas-esquerda .fusion-li-item li" ).index( this );
                
                $container.find('.abas-duas-colunas-esquerda .fusion-li-item li').removeClass("active");
                jQuery(this).addClass("active");
                jQuery(desc).css("display","none");
                jQuery(desc).eq(index).css("display","block");
            });
        } else {
            $container.find('.fusion-li-item').on("click", function(e){
                var desc = $container.find('.abas-duas-colunas-direita .fusion-builder-row'),
                    index = $container.find(".abas-duas-colunas-esquerda .fusion-li-item" ).index( this );
                
                $container.find('.abas-duas-colunas-esquerda .fusion-li-item').removeClass("active");
                jQuery(this).addClass("active");
                jQuery(desc).css("display","none");
                jQuery(desc).eq(index).css("display","block");
            });
        }
    });
}

if(jQuery('.formulario-em-linha').length > 0) {
    var $botao = jQuery(".exibe-formulario");
    var $form = jQuery(".formulario-em-linha");
    $form.hide();
    jQuery(document).on("click", ".exibe-formulario", function(){
        $botao.slideUp("500", function(){
            $form.slideDown("500");
        });
    });
}

if(jQuery('.barra-fixa').length > 0) {
    jQuery('.barra-fixa').clone().addClass("clone").appendTo("body");
    jQuery('.barra-fixa.clone').find("[class^='title-heading']").text(document.title.replace(" ‚Äì Instituto Anima",""));
    jQuery(window).on("scroll", function(e){
        var posicaoBarra = jQuery(".barra-fixa:not(.clone)").outerHeight() + jQuery(".barra-fixa:not(.clone)").offset().top - jQuery(".fusion-header-wrapper").outerHeight();
        var documentPos = document.body.getBoundingClientRect().top * -1;
        if(documentPos >= posicaoBarra) {
            jQuery('.barra-fixa.clone').addClass("ativa");
        } else {
            jQuery('.barra-fixa.clone').removeClass("ativa");
        }
    });
}

